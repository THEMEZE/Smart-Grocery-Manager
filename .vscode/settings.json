{
  "liveServer.settings.port": 5502,
  "editor.formatOnSave": true,
  "editor.autoIndent": "full",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,

  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.tabSize": 4
  },

  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
